package flat/primitive/number

testable class {
  test `toEnglish can get number less than 10`() {
    expect(Long.toEnglish(5)).toBe("5")
  }

  test `toEnglish can get number less than 100`() {
    expect(Long.toEnglish(15)).toBe("15")
  }

  test `toEnglish can get number less than 1000`() {
    expect(Long.toEnglish(150)).toBe("150")
  }

  test `toEnglish can get number equals 999`() {
    expect(Long.toEnglish(999)).toBe("999")
  }

  test `toEnglish can get number equals 1000`() {
    expect(Long.toEnglish(1000)).toBe("1,000")
  }

  test `toEnglish can get number equals 10000`() {
    expect(Long.toEnglish(10000)).toBe("10,000")
  }

  test `toEnglish can get number equals 100000`() {
    expect(Long.toEnglish(100000)).toBe("100,000")
  }

  test `toEnglish can get number equals 1000000`() {
    expect(Long.toEnglish(1000000)).toBe("1,000,000")
  }

  test `toEnglish can get number equals 10000000`() {
    expect(Long.toEnglish(10000000)).toBe("10,000,000")
  }

  test `toEnglish can get number equals 100000000`() {
    expect(Long.toEnglish(100000000)).toBe("100,000,000")
  }

  test `toEnglish can get number equals 1000000000`() {
    expect(Long.toEnglish(1000000000)).toBe("1,000,000,000")
  }

  test `toEnglish can get negative number less than 0`() {
    expect(Long.toEnglish(-5)).toBe("-5")
  }

  test `toEnglish can get negative number less than -10`() {
    expect(Long.toEnglish(-15)).toBe("-15")
  }

  test `toEnglish can get negative number less than -100`() {
    expect(Long.toEnglish(-150)).toBe("-150")
  }

  test `toEnglish can get negative number equals negative 999`() {
    expect(Long.toEnglish(-999)).toBe("-999")
  }

  test `toEnglish can get negative number equals negative 1000`() {
    expect(Long.toEnglish(-1000)).toBe("-1,000")
  }

  test `toEnglish can get negative number equals negative 10000`() {
    expect(Long.toEnglish(-10000)).toBe("-10,000")
  }

  test `toEnglish can get negative number equals negative 100000`() {
    expect(Long.toEnglish(-100000)).toBe("-100,000")
  }

  test `toEnglish can get negative number equals negative 1000000`() {
    expect(Long.toEnglish(-1000000)).toBe("-1,000,000")
  }

  test `toEnglish can get negative number equals negative 10000000`() {
    expect(Long.toEnglish(-10000000)).toBe("-10,000,000")
  }

  test `toEnglish can get negative number equals negative 100000000`() {
    expect(Long.toEnglish(-100000000)).toBe("-100,000,000")
  }

  test `toEnglish can get negative number equals negative 1000000000`() {
    expect(Long.toEnglish(-1000000000)).toBe("-1,000,000,000")
  }
}